<h1>All Your Favorite Restaurants</h1>

<div class = "profile-restaurants"
    <ul>
    <% results.forEach(restaurant =>{ %>
        <li><a href = "travelBuddy/<%=restaurant.id%>"><%= restaurant.name %></a></li>
        <%= restaurant.restaurantId%>  <br>
        <%= restaurant.id%>
        <form method = "POST" action= "/profile/restaurants/<%= restaurant.id%>?_method=DELETE">
          
          <button type="submit" class="btn btn-danger">Remove From Faves</button>

        </form> 

        
  <%  })%>
    </ul>

